{
    "configKey": "{{ resource_template.parameters.azure_configuration_entry_config_key }}",
    "configValueFileName": "{{ resource_template.parameters.azure_configuration_entry_config_name | append: ".configurationentry.json" }}",
    "configLabel" : "ConfigurationEntry",
    "configStoreName": "{{ resource_template.parameters.azure_config_store }}",
    "configType": "application/json",
    "configTags": [
{%- for tag in resource_template.tags -%}
    {%- if forloop.last == false %}
        "{{ tag.key }}={{ tag.value }}",
    {%- else %}
        "{{ tag.key }}={{ tag.value }}"
    {%- endif -%}
{%- endfor %}
    ]
}