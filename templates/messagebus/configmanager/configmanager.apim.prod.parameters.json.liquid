{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "apimServiceName": {
            "value": "{{ resource_template.resource_name }}"
        },
        "apiName": {
            "value": "{{ resource_template.parameters.api_name }}"
        },
        "configurationManagerAppConfigName": {
            "value": "{{ resource_template.parameters.app_config_name }}"
        },
        "configurationManagerResourceGroupName": {
            "value": "{{ resource_template.parameters.azure_message_bus_resource_group_name }}"
        },
        "artifactStoreName": {
            "value": "{{ resource_template.parameters.azure_integration_account_name }}"
        },
        "artifactStoreResourceGroupName": {
            "value": "{{ resource_template.parameters.azure_message_bus_resource_group_name }}"
        },
        "cacheTimeout": {
            "value": "3600"
        }
    }
}